{
  "name": "open-doc-demo",
  "version": "1.0.0",
  "repository": "git@github.com:flut1/open-doc-demo.git",
  "main": "index.js",
  "scripts": {
    "fix": "eslint . --ext js,mdx --fix",
    "lint": "eslint . --ext js,mdx",
    "render": "npm run render:html && npm run render:pdf",
    "render-artifacts": "node scripts/render.js --artifacts && npm run render:pdf",
    "render:html": "node scripts/render.js",
    "render:pdf": "node node_modules/puppeteer-core/install.js && node scripts/print.js",
    "start": "nodemon scripts/dev.js",
    "watch": "node scripts/watch.js"
  },
  "config": {
    "PUPPETEER_PRODUCT": "chrome"
  },
  "nodemonConfig": {
    "watch": ["scripts/**"]
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.14",
    "core-js": "^3.6.5",
    "node-eval": "^2.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rollup-plugin-mdx": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@mdx-js/mdx": "^1.6.14",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/pluginutils": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "chokidar": "^3.4.1",
    "date-fns": "^2.15.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mdx": "^1.7.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "git-last-commit": "^1.0.0",
    "js-beautify": "^1.11.0",
    "mustache": "^4.0.1",
    "nodemon": "^2.0.4",
    "open": "^7.1.0",
    "prettier": "^2.0.5",
    "puppeteer-core": "^5.2.1",
    "rollup": "^2.23.0"
  },
  "engines": {
    "node": ">=10.21.0"
  }
}
