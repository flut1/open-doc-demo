{
  "name": "open-doc-demo",
  "version": "1.0.0",
  "repository": "git@github.com:flut1/open-doc-demo.git",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write \"(src|scripts)/**.(js|mdx)\"",
    "render": "npm run render:html && npm run render:pdf",
    "render-artifacts": "node ./scripts/render.js --artifacts && npm run render:pdf",
    "render:html": "node ./scripts/render.js",
    "render:pdf": "node ./scripts/print.js",
    "start": "node ./scripts/dev.js",
    "watch": "node ./scripts/watch.js --run"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@mdx-js/mdx": "^1.6.14",
    "@mdx-js/react": "^1.6.14",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "date-fns": "^2.15.0",
    "fs-extra": "^9.0.1",
    "git-last-commit": "^1.0.0",
    "has-flag": "^4.0.0",
    "js-beautify": "^1.11.0",
    "mustache": "^4.0.1",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.23.0",
    "rollup-plugin-mdx": "^0.1.0"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "prettier": "^2.0.5",
    "rollup-plugin-node-externals": "^2.2.0"
  },
  "engines": {
    "node": ">=12.4.0"
  }
}
